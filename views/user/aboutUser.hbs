<div class="aboutUser">
	<div class="userInfo">
		<div class="userImg">
			<img src="{{photo}}" alt="">
		</div>
		<div class="userInfoText">
			<h3>{{username}}</h3>
		</div>
	</div>
	<div class="fotos">
		{{#each fotos}}
			<div class="foto">
				<img src="{{img}}" alt="">
				<a href="/user/detail/{{_id}}">Detail</a>
			</div>
		{{/each}}
	</div>
</div>